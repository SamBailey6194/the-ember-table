{
  "name": "theme",
  "version": "4.0.1",
  "description": "",
  "scripts": {
    "start": "npm run dev",
    "build": "npm run build:clean && npm run build:tailwind",
    "build:clean": "rimraf ../../static/css/style.css",
    "build:tailwind": "cross-env NODE_ENV=production postcss ./src/style.css -o ../../static/css/style.css",
    "dev": "cross-env NODE_ENV=development postcss ./src/style.css -o ../../static/css/style.css --watch",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "cross-env": "^7.0.3",
    "jest": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "postcss-nested": "^7.0.2",
    "postcss-simple-vars": "^7.0.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.0"
  },
  "dependencies": {
    "daisyui": "^5.1.29"
  }
}
